<div class="section no-pad-bot" id="index-banner">
  <div class="container">
      <div class="col-lg-1 mapContainer">
          <div id="map"></div>
      </div>
  </div>
</div>
<div class="fixed-action-btn toolbar">
  <a class="btn-floating btn-large orange">
    <i class="large material-icons">apps</i>
  </a>
  <ul>
    <li class="waves-effect waves-light">
      <%=  link_to  new_idea_path, remote: true do %>
        <i class="material-icons">lightbulb_outline</i>
        <% end %>
    </li>
    <li class="waves-effect waves-light">
      <%=  link_to  new_contest_path, remote: true do %>
        <i class="material-icons">search</i>
        <% end %>
    </li>
    <li class="waves-effect waves-light">
        <%=  link_to  contests_path do %>
          <i class="material-icons">search</i>
          <% end %>
      </li>
      <li class="waves-effect waves-light">
      <a href="#!">
        <i class="material-icons">pin_drop</i>
      </a>
    </li>
    <li class="waves-effect waves-light">
      <a href="#!">
        <i class="material-icons">star_border</i>
      </a>
    </li>
  </ul>
</div>
<div class="container">
  <div class="section">
      <div id="modal1" class="modal modal-fixed-footer">
        </div>
        <div id="modal2" class="modal modal-fixed-footer">
        </div>
        <div id="modal3" class="modal modal-fixed-footer">
        </div>
    <h3 class="center">O que a Cidade Manda ?</h3>
    <div id="ideas">
        <%= render @ideas%>
        
      </div>   
      <h3 class="center">O que querem saber Cidade ?</h3> 
    <div id="contests">
      <%= render @contests%>
    </div>
   
</div>
